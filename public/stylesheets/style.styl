body
  font-family "Helvetica Neue", "Lucida Grande", "Arial"
  font-size 13px
  text-align center
  text-stroke 1px rgba(255, 255, 255, 0.1)
  color #555
h1, h2
  margin 0
  font-size 22px
  color #343434
h1
  text-shadow 1px 2px 2px #ddd
  font-size 60px
#sensors
  text-align left
  margin-left auto
  margin-right auto
  width 640px
  .sensor
    margin 20px
    padding 5px
    .title
        font-weight bold
        background-color #989898
    .body
        background-color #898989